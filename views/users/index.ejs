<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <header>
      <h1>Profile Page</h1>
      <h3>Welcome</h3>
      <nav>
        <ul>
          <li>
            <a href="/">Home</a>
          </li>
          <li>
            <a href="/users/new">Create New User</a>
          </li>
        </ul>
      </nav>
    </header>
      <main>
        <h2>Users</h2>
        <ul>
      <%  for ( let i = 0; i < users.length; i++ ) { %>
        <li>
          <%=  users[i].name %> </br>

          <img src="<%=users[i].profilePic%>"/></br>

          <form action="/users/<%=users[i].id%>" method="GET">
            <input type="submit" value="View User">

        </form>
      </li>
    <%    } %>
  </ul>
     <main>

      </main>
  </body>
</html>
